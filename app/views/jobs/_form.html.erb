<%= form_for [@job] do |f| %>
  <%= f.label :role %>
  <%= f.text_field :title %>

  <%= f.label :company %>
  <%= f.text_field :company_id %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :level_of_interest %>
  <%= f.number_field :level_of_interest %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.submit %>
<% end %>
